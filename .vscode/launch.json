{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost/freecodecamp/codepen/weatherapp/",
      "webRoot": "${workspaceRoot}",
      "userDataDir": false
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceRoot}/index.html"
    }
  ]
}